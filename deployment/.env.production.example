# Example production environment for docker-compose.2.8.4-production.yml
# Copy to deployment/.env (recommended) or pass with --env-file

# Domain and contact email for Let's Encrypt
DOMAIN=zionterranova.com
LETSENCRYPT_EMAIL=admin@zionterranova.com

# Optional: enable Let's Encrypt staging (0=prod, 1=staging)
LETSENCRYPT_STAGING=0

# ZION runtime (can override compose defaults)
ZION_ENV=production
ZION_VERSION=2.8.4
ZION_LOG_LEVEL=info
ZION_DEFAULT_ALGO=cosmic_harmony
